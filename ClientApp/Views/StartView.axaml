<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:vm="using:ClientApp.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:DataType="vm:StartViewModel"
             x:Class="ClientApp.Views.StartView">
	
	<StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
		<Button Content="Войти" Command="{Binding LoginCommand}" Margin="0, 25" Width="200" HorizontalContentAlignment="Center"/>
		<Button Content="Регистрация" Margin="0, 25" Width="200" HorizontalContentAlignment="Center"
				Command="{Binding RegistartionCommand}"/>
	</StackPanel>
</UserControl>
